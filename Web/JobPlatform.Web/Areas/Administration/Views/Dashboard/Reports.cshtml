@{
    this.ViewData["Title"] = "Reports";
}

<div>
    <div class="row">
        <div class="col-md-3">
            <partial name="_ManageNav" />
        </div>
        <div class="col-md-9">
            @if (this.TempData["InfoMessage"] != null)
            {
                <div class="alert alert-success">
                    @this.TempData["InfoMessage"]
                </div>
            }
            <h1>@this.ViewData["Title"]</h1>
            <partial name="_ReportBrowsePartial" />
        </div>
    </div>
</div>
